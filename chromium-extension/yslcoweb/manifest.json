{
     "name": "YSLcoWeb-lite",
     "version": "1.0",
     "manifest_version": 3,
     "description": "Web assistant by YSL",
     "permissions": ["tabs", "activeTab"],
     "icons": {
          "16": "icon16.png",
          "48": "icon48.png",
          "128": "icon128.png"
     },
     "action": {
          "default_title": "Open YSLcoWeb",
          "default_icon": "icon128.png",
          "default_popup": "popup.html"
     }
}
